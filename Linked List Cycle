/**
 * Definition for singly-linked list.
 * class ListNode {
 *     int val;
 *     ListNode next;
 *     ListNode(int x) {
 *         val = x;
 *         next = null;
 *     }
 * }
 */
public class Solution {
    public boolean hasCycle(ListNode head) {
        if(head==null) return false;
        HashSet hs = new HashSet();
        hs.add(head);
        while(head.next!=null){
            if(hs.contains(head.next)){
                return true;
            }
            head = head.next;
            hs.add(head);
        }
        return false;
    }
}
